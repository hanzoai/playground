# Production environment variables for Playground UI

# API Configuration
VITE_API_BASE_URL=/v1

# Build Configuration
VITE_BASE_PATH=/
VITE_BUILD_OUT_DIR=dist
VITE_BUILD_SOURCEMAP=false

# Application Configuration
VITE_APP_TITLE=Playground Server Dashboard
VITE_APP_DESCRIPTION=Playground AI Agent Orchestration Platform

# IAM Configuration (enables IAM auth mode with org/project switching)
VITE_IAM_SERVER_URL=https://hanzo.id
VITE_IAM_CLIENT_ID=hanzobot-client-id

# Bot Gateway URL (for embedded Control UI iframe)
VITE_BOT_GATEWAY_URL=https://gw.hanzo.bot

# Gateway WebSocket URL (for canvas live connection)
VITE_GATEWAY_URL=wss://gw.hanzo.bot

# Commerce API URL (for billing balance and credit operations)
VITE_COMMERCE_API_URL=https://commerce.hanzo.ai
