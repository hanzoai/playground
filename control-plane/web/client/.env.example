# Playground UI Environment Variables
# Copy this file to .env.local and modify values as needed

# API Configuration
VITE_API_BASE_URL=/api/ui/v1

# Development Server Configuration
VITE_DEV_PORT=5173
VITE_DEV_HOST=localhost
VITE_API_PROXY_TARGET=http://localhost:8080

# Build Configuration
VITE_BASE_PATH=/ui/
VITE_BUILD_OUT_DIR=dist
VITE_BUILD_SOURCEMAP=false

# Application Configuration
VITE_APP_TITLE=Playground Server Dashboard
VITE_APP_DESCRIPTION=Playground AI Agent Orchestration Platform
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
# VITE_ENABLE_DEBUG=false
# VITE_ENABLE_ANALYTICS=false
